{
  "name": "express-typescript",
  "version": "0.0.1",
  "description": "Dockerized Express Typescript",
  "license": "ISC",
  "author": "Mohammed Mahmoud",
  "main": "src/app.ts",
  "scripts": {
    "client:build": "cd src/client && node scripts/build.js",
    "client:start": "cd src/client && node scripts/start.js",
    "client:test": "cd client && node scripts/test.js",
    "dev": "concurrently \"yarn server:start\" \"yarn client:start\"",
    "migrate": "migrate-mongo up",
    "mongodb:rm": "yarn mongodb:stop && docker rm $(docker ps -aqf \"name=^mongo$\")",
    "mongodb:start": "docker-compose --project-name mongo --file cicd/mongodb.yaml up -d",
    "mongodb:stop": "docker stop $(docker ps -aqf \"name=^mongo$\")",
    "server:build": "webpack",
    "server:prod": "yarn server:build && yarn mongodb:start && node ./dist/api.bundle.js",
    "server:prod:docker": "docker-compose --file cicd/deployment.yaml up --build -d",
    "server:start": "nodemon --watch src/server --exec ts-node src/server/app.ts",
    "test": "jest --detectOpenHandles --testPathPattern=src/server",
    "test:coverage": "jest --collect-coverage --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "type-check": "tsc --noEmit"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "modulePaths": [],
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "dependencies": {
    "antd": "^4.5.1",
    "compression": "^1.7.4",
    "connect-timeout": "^1.9.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-expand": "5.1.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-json-validator-middleware": "^2.1.1",
    "express-mung": "^0.5.1",
    "faker": "^4.1.0",
    "helmet": "^3.23.0",
    "lodash": "^4.17.15",
    "migrate-mongo": "^8.1.1",
    "moment": "^2.27.0",
    "mongoose": "^5.9.18",
    "mongoose-paginate-v2": "^1.3.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.1.0",
    "yup": "^0.29.2"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@svgr/webpack": "4.3.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/compression": "^1.7.0",
    "@types/connect-timeout": "^0.0.34",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-mung": "^0.5.2",
    "@types/helmet": "^0.0.47",
    "@types/jest": "^26.0.5",
    "@types/lodash": "^4.14.155",
    "@types/mongoose": "^5.7.24",
    "@types/mongoose-paginate": "^5.0.8",
    "@types/mongoose-paginate-v2": "^1.3.6",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/supertest": "^2.0.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.0.0",
    "@types/yup": "^0.29.3",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "concurrently": "^5.2.0",
    "css-loader": "3.4.2",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "4.3.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "identity-obj-proxy": "3.0.0",
    "jest": "^26.1.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "mini-css-extract-plugin": "0.9.0",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^10.2.1",
    "resolve": "1.15.0",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "8.0.2",
    "semver": "6.3.0",
    "style-loader": "0.23.1",
    "supertest": "^4.0.2",
    "terser-webpack-plugin": "2.3.5",
    "ts-jest": "^26.1.3",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "ts-pnp": "1.1.6",
    "typescript": "^3.9.7",
    "url-loader": "2.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "2.2.0",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "4.3.1"
  }
}
